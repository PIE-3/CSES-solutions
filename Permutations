#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define vi vector<int>
#define vll vector<long long int>
#define mod 1e9+7
#define ff first
#define ss second
#define ins insert
#define endl "\n"
#define pie_op ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0)
#define tc int t;cin>>t;while(t--)
#define pb push_back
#define input(n) int arr[n];for(int i1=0;i1<n;i1++)cin>>arr[i1]
#define lb(x) lower_bound(x)
#define ub(x) upper_bound(x)
#define bp(x) __builtin_popcount(x)
 
int main(){
	pie_op;
	int n;
	cin >> n;
	if(n == 1)
	cout << 1;
	else if(n < 4) cout << "NO SOLUTION";
	else if(n == 4) cout << "3 1 4 2";
	else{
		for(int i = 1; i <= n; i += 2){
			cout << i << " ";
		}
		for(int i = 2; i <= n; i += 2){
			cout << i << " ";
		}
	}
	return 0;
}
